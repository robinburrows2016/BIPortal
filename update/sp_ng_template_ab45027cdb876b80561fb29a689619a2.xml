        <template><![CDATA[              <div class="modal-content">
                <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal" ng-click="c.clearComment()" aria-label="{{data.messages.CLOSE}}"><i class="fa fa-times" aria-hidden="true"></i></button>
                  <h4 id="flagTitle" class="modal-title">{{::data.messages.FLAG_THIS_ARTICLE}}</h4>
                </div>
                  <div class="modal-body">
                    <textarea aria-required="true" ng-required="true" aria-label="{{data.messages.ADD_COMMENT}}" placeholder="{{data.messages.ADD_COMMENT}}" ng-model="::data.comment" class="form-control" rows="5" id="flagComment"></textarea>
                    <div role="alert" aria-live="assertive" class="alert-danger" ng-shown="!c.flagMessage">{{c.flagMessage}}</div>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="c.clearComment()">{{data.messages.CANCEL}}</button>
                    <button type="button" class="btn btn-primary" aria-label="{{!data.comment ? c.submitFlagWithoutComment : c.submitFlagWithComment }}" data-dismiss="modal" ng-click="c.submitFlagComments()">{{data.messages.SUBMIT}}</button>
                  </div>                  

              </div>]]></template>
    </sp_ng_template>
