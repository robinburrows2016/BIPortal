        <sys_updated_on>2019-02-26 14:14:02</sys_updated_on>
        <template><![CDATA[<div class="tooltip-popover" id="tooltip-popover-{{a.sys_id}}">
  <div class="popover-title">
    {{a.number}} - {{a.short_description}}
    <p>
      <small>
        <span ng-if="a.confidential == 'false'" class="author pad-right text-nowrap"><i class="fa fa-user pad-right" />{{a.author}}</span>
        <span class="published pad-right text-nowrap"><i class="fa fa-calendar pad-right" /><sn-time-ago timestamp="a.updated_on"/></span>
        <span class="views pad-right text-nowrap">
          <span class="pad-right">&#8226;</span>
          <i class="fa fa-eye pad-right" />
            ${{{a.viewCount}} Views}
        </span>
      </small>
    </p>    
  </div>
  <ul class="color-items">
    <li class="color-item">
      <h5>Description</h5>
	  <div class="color-label">{{a.description}}</div>
      <h5>Benefits</h5>      
      <div class="color-label">{{a.business_case}}</div>      
    </li>    
  </ul>  
</div>]]></template>
    </sp_ng_template>
</record_update>
